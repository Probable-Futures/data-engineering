{
  "compilerOptions": {
    "baseUrl": "tests",
    "outDir": "dist",
    "allowJs": false,
    "target": "ES6",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "sourceMap": false,
    "listEmittedFiles": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["tests/*", "tiles/*", "woodwellDatasets/*"],
  "exclude": ["node_modules"]
}
